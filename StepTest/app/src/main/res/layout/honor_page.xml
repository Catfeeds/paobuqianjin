<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:id="@+id/table_layout_honor"
        android:layout_width="match_parent"
        android:layout_height="64dp"
        android:background="@color/color_232433">

        <android.support.design.widget.TabLayout
            android:id="@+id/person_circle_nearby"
            android:layout_width="match_parent"
            android:layout_alignParentBottom="true"
            android:layout_height="44dp"
            app:tabIndicatorColor="@color/color_feb913"
            app:tabIndicatorHeight="2dp"
            app:tabSelectedTextColor="@color/color_feb913"
            app:tabTextAppearance="@style/MyTabLayoutTextAppearanceStyle_17"
            app:tabTextColor="@color/color_8a8a8a" />
    </RelativeLayout>

    <com.paobuqianjin.pbq.step.view.base.adapter.UnScrollViewPager
        android:id="@+id/honor_viewpage"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/table_layout_honor"></com.paobuqianjin.pbq.step.view.base.adapter.UnScrollViewPager>


</RelativeLayout>